<?xml version='1.0'?>

<?xml-stylesheet href="../foo.xsl" type="text/xsl"?>

<foo
    xmlns="http://foo.example.org"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:xh="http://www.w3.org/1999/xhtml"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:bar="https://bar.example.info/"
    xmlns:baz="https://baz.example.edu#"
    xml:lang="is"
    owl:sameAs="foo.json"
    xsi:nil="false"
    xsi:type="bar:bar"
    baz="BAZ"
    baz:special="">
  <xh:p about="[bar:foo]" xml:lang="fo" lang="da">
    <baz/>

    <xh:span xml:id="xh1" lang="no">
      <xh:span xml:id="xh2"/>

      <xh:span xml:lang="fi">
        <xh:span xml:id="xh3"/>
      </xh:span>
    </xh:span>
  </xh:p>

  <bar baz:special="Lorem ipsum"/>
</foo>
