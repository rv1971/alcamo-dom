<?xml version="1.0" encoding="UTF-8"?>

<xsd:schema
    xmlns:dc="http://purl.org/dc/terms/"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema"
    xmlns:bar="https://bar.example.com#"
    targetNamespace="http://foo.example.org"
    attributeFormDefault="qualified"
    xml:lang="en">
  <xsd:notation
      name="FooNotation"
      id="FooNotation"
      public="application/x-foo"
      system="http://foo.example.org/FooData">
    <xsd:annotation>
      <xsd:appinfo xmlns="http://www.w3.org/1999/xhtml">
        <meta property="bar:fooish" content="false"/>
        <meta property="bar:fooable" content="true"/>
        <link
            rel="dc:seeAlso"
            href="http://foo.example.org/documentation/FooNotation"/>
        <link
            rel="dc:hasVersion"
            href="http://foo.example.org/FooNotation/coolVersion"/>
        <link
            rel="dc:hasVersion"
            href="http://foo.example.org/FooNotation/uncoolVersion"/>
      </xsd:appinfo>
    </xsd:annotation>
  </xsd:notation>
</xsd:schema>
