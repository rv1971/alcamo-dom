  <xsd:import
      namespace="http://www.w3.org/2001/XMLSchema"
      schemaLocation="XMLSchema.xsd"/>

  <xsd:attribute name="sameAs" type="xsd:anyURI"/>

  <xsd:attribute name="versionInfo" type="xsd:string" rdfs:label="Version"/>
